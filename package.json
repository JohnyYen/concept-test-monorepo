{
  "dependencies": {
    "@nestjs/mapped-types": "*"
  },
  "name": "fullstack-monorepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "fix:api": "node scripts/fix-imports.js",
    "dev:web": "pnpm dev --filter api web",
    "generate:api": "openapi-generator-cli generate -i ./packages/api-contract/openapi.yml -g typescript-nestjs-server -o ./packages/api-stubs/ && node scripts/fix-imports.js",
    "generate:mobile": "openapi-generator-cli generate -i ./packages/api-contract/openapi.yml -g dart -o ./packages/dart-client/",
    "generate:web": "openapi-generator-cli generate -i ./packages/api-contract/openapi.yml -g typescript-fetch -o ./packages/nextjs-client/",
    "mock:export": "mockoon-cli export -i ./packages/api-contract/openapi.yml -o ./packages/mock-server/data.json",
    "mock:run": "mockoon-cli start --data ./packages/mock-server/data.json --port 3500"
  },
  "devDependencies": {
    "@mockoon/cli": "^9.5.0",
    "@openapitools/openapi-generator-cli": "^2.29.0",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "@workspace/api-stubs": "workspace:*",
    "prettier": "^3.8.1",
    "turbo": "^2.8.1",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  }
}
